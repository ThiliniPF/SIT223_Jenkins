pipeline
{
    agent any
    stages
    {
        stage('Build')
        {
            steps
            {
                sh 'mvn clean package'
            }
        }
        stage('Unit and Integration Tests')
        {
            steps
            {
                sh 'mvn test'
                sh 'mvn integration-test'
            }
        }
        stage('Code Analysis')
        {
            steps
            {
                withMaven(maven: 'maven_1298.v43b_82f220a_e9')

            }
        }
    }
}
